func is(expect, got, name) {
  if (expect == got) {
    println(name + ":", "OK")
  } else {
    println(name + ":", "NG")
    throw name + ": expected " + expect + " but got " + got
  }
}

func ok(expect, name) {
  if (expect) {
    println(name + ":", "OK")
  } else {
    println(name + ":", "NG")
    throw name + ": expected " + expect + " but got " + got
  }
}

